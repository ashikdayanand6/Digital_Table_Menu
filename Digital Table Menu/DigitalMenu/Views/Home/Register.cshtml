<!DOCTYPE html>
<html>
<head>
    <title>Digital Menu</title>
    <!-- metatags-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Transitive register form a Flat Responsive Widget,Login form widgets, Sign up Web forms , Login signup Responsive web form,Flat Pricing table,Flat Drop downs,Registration Forms,News letter Forms,Elements" />
    <script type="application/x-javascript">
         addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
        function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Meta tag Keywords -->
    <link href="@Url.Content("~/Content/Register/")css/style.css" rel="stylesheet" type="text/css" media="all" />
    <link rel="stylesheet" href="@Url.Content("~/Content/Register/")css/font-awesome.css"><!--font_wesome_icons-->
    <link href="//fonts.googleapis.com/css?family=Exo+2" rel="stylesheet"><!--online fonts-->
    <link href="//fonts.googleapis.com/css?family=PT+Sans" rel="stylesheet"><!--online fonts-->
</head>
<body>
    <div class="w3ls-headding">
        <h1><span></span> <span>r</span>egister <span>f</span>orm</h1>
    </div>
    <div class="agile-main">
     
        <div class="agile-right">
            <form action="Register" method="post" enctype="multipart/form-data">
                <div class="agile-right-h2">
                    <h2> register here</h2>
                </div>
                <div class="w3l-name">
                    <span><i class="fa fa-user" aria-hidden="true"></i></span>
                    <input type="text" name="name" placeholder="Username" required="" />
                </div>
                <div class="clear"></div>
                <div class="w3l-email">
                    <span><i class="fa fa-desktop" aria-hidden="true"></i></span>
                    <input type="text" name="address" placeholder="Address" required="" />
                </div>
                <div class="clear"></div>
                <div class="w3l-email">
                    <span><i class="fa fa-lock" aria-hidden="true"></i></span>
                    <input type="text" name="city" placeholder="City" required="" />
                </div>
                <div class="clear"></div>
                <div class="w3l-email">
                    <span><i class="fa fa-lock" aria-hidden="true"></i></span>
                    <input type="text" name="mobile" placeholder="Mobile No." required="" />
                </div>
                <div class="clear"></div>
                <div class="w3l-email">
                    <span><i class="fa fa-envelope" aria-hidden="true"></i></span>
                    <input type="email" name="email" placeholder="Email" required="" />
                </div>
                <div class="clear"></div>
                <div class="w3l-psw">
                    <span><i class="fa fa-lock" aria-hidden="true"></i></span>
                    <input type="password" name="password" placeholder="password" required="" />
                </div>
                <div class="clear"></div>
                <div class="w3l-psw">
                    <span><i class="fa fa-lock" aria-hidden="true"></i></span>
                    <input type="file" name="file" id="file" required="" />
                </div>
                <div class="clear"></div>
                <div class="w3l-cpsw">
                    <span><i class="fa fa-lock" aria-hidden="true"></i></span>
                    <select id="typ" name="typ" required="">
                        <option value="">--Select Type--</option>
                        <option value="Farmer">Farmer</option>
                        <option value="Retailer">Retailer</option>
                        <option value="User">User</option>
                    </select>
                </div>
                <div class="w3l-agree">
                   
                </div>
                <div class="clear"></div>

                <div class="w3l-submit">
                    <input type="submit" value="register now">
                </div>
            </form>
            
            <div class="wthree-social">
                <a href="/Home/Index"><i aria-hidden="true"></i> Home Page </a>
            </div>
        </div>
        <div class="clear"></div>
    </div>
    <footer>
        <p>&copy; Register Form. All Rights Reserved | <a href="#"> </a></p>
    </footer>
    <script>
        $(function () {
            var suc = '@TempData["RegisterSc"]'

            if(suc!='')
                toastr.success(suc);
        });
    </script>
</body>
</html>
